#!usr/bin/env sh
while true;
do
x=`batteryCondition | grep "State\ of\ Charge" |  awk '{print $5}'`
date1=$(date +%s)
date2=
#echo $date1
if [ $x -lt 15 ];
then
	date2=$(date +%s)
	echo $(($date2-$date1))
	echo $(( (date2 - date1)/86400))
	while true;
	do
		tput bel
		sleep 1
	done
fi
sleep 300
done
